<div class="card">
    <div class="card-header">
        <h3 class="card-title">{{ competitionName }}</h3>
        <p class="card-text">Prediction rounds.</p>
    </div>
    <div class="card-body">
        <ul class="list-group list-group-flush">
            {% for round, value in roundInfo %}
                <li class="list-group-item">
                    <a class="card-link" href="{{ path('app_prediction_round', {'id': competitionId, 'round': round}) }}">
                        <div class="circle circle-{{ value.status|lower }}"><div class="status-{{ value.status|lower }}"></div></div>
                        {% if round matches '/^\\d+$/' %}
                            {{ 'Round #' ~ round}}
                        {% else %}
                            {{ round }}
                        {% endif %}
                        {{ value.date }}
                    </a>
                </li>
            {% endfor %}
        </ul>
    </div>
</div>